	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Script para generar valores dinámicos para proveedores",
					"if (!pm.collectionVariables.get('proveedor_idempotency_key')) {",
					"    pm.collectionVariables.set('proveedor_idempotency_key', pm.variables.replaceIn('{{$guid}}'));",
					"}",
					"",
					"// Generar datos aleatorios para proveedores si no existen",
					"if (!pm.collectionVariables.get('proveedor_nit')) {",
					"    const timestamp = new Date().getTime();",
					"    pm.collectionVariables.set('proveedor_razon_social', 'Proveedor Test ' + timestamp);",
					"    pm.collectionVariables.set('proveedor_nit', '900' + timestamp.toString().slice(-6));",
					"    pm.collectionVariables.set('proveedor_email', 'test.' + timestamp + '@example.com');",
					"    pm.collectionVariables.set('proveedor_telefono', '+57-1-' + Math.floor(Math.random() * 10000000));",
					"    pm.collectionVariables.set('proveedor_direccion', 'Calle Test ' + Math.floor(Math.random() * 1000));",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Tests automáticos para verificar respuestas",
					"pm.test('Response time is less than 2000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(2000);",
					"});",
					"",
					"pm.test('Status code is handled appropriately', function () {",
					"    // Verificar códigos de estado específicos",
					"    switch (pm.response.code) {",
					"        case 200:",
					"        case 201:",
					"            pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
					"            break;",
					"        case 400:",
					"            pm.expect(pm.response.json()).to.have.property('detail');",
					"            break;",
					"        case 409:",
					"            pm.expect(pm.response.json().detail).to.include('NIT duplicado');",
					"            break;",
					"    }",
					"});",
					"",
					"// Guardar NIT existente para pruebas de duplicados",
					"if (pm.response.code === 201) {",
					"    const responseData = pm.response.json();",
					"    if (responseData.nit) {",
					"        pm.collectionVariables.set('proveedor_nit_existente', responseData.nit);",
					"    }",
					"}"
				]
			}
		}
	],
